{
  "manifest_version": 3,
  "name": "Ready64 Magazine Scraper",
  "version": "1.0",
  "description": "Scrapes magazine data from ready64.org",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://ready64.org/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
